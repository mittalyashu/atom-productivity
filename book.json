{
    "plugins": [
      "anchorjs",
      "edit-link"
    ],
    "language": "en",
    "pluginsConfig": {
      "anchorjs": {
        "selector": "h2,h3,h4,h5",
        "icon": "#",
        "placement": "left",
        "visible": "always",
        "class": "custom-class another-one",
        "truncate": 64,
        "ariaLabel": "Anchor"
      },
      "edit-link": {
        "base": "https://github.com/mittalyashu/atom-productivity/edit/BRANCH/path/to/book",
        "label": "Edit This Page"
      }
  }
}