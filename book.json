{
    "plugins": [
      "anchorjs",
      "github-edit",
      "alerts",
      "insert-logo-link"
    ],
    "language": "en",
    "pluginsConfig": {
      "anchorjs": {
        "selector": "h1,h2,h3,h4,h5",
        "icon": "#",
        "placement": "left",
        "visible": "always",
        "class": "custom-class another-one",
        "truncate": 64,
        "ariaLabel": "Anchor"
      },
      "github-edit": {
        "repo": "mittalyashu/atom-productivity",
        "branch": "master"
      },
      "insert-logo-link": {
        "src": "https://i.imgur.com/Ub2d05u.png",
        "url": "https://www.codecarrot.net/"
      }
  }
}