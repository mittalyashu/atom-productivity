{
    "plugins": [
      "anchorjs",
      "github-edit"
    ],
    "language": "en",
    "pluginsConfig": {
      "anchorjs": {
        "selector": "h1,h2,h3,h4,h5",
        "icon": "#",
        "placement": "left",
        "visible": "always",
        "class": "custom-class another-one",
        "truncate": 64,
        "ariaLabel": "Anchor"
      },
      "github-edit": {
        "repo": "mittalyashu/atom-productivity",
        "branch": "master"
      }
  }
}